extends layouts/main.pug
include mixins/videoBlock

block content
    div.search__header
        h3 Searching for: #{searchingBy}
        if videos.length === 0
            h5 No Videos Found
        each item in videos
            +videoBlock({
                id: item.id,
                title: item.title,
                views: item.views,
                videoFile: item.videoFile
            })